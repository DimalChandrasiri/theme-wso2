{
  "name": "theme-wso2",
  "display-name": "WSO2 UI Components",
  "version": "3.0.0",
  "description": "Theme WSO2 React Node Module + Domumentation",
  "private": false,
  "author": "WSO2",
  "repository": {
    "type": "git",
    "url": "https://github.com/wso2-dev-ux/theme-wso2.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wso2-dev-ux/theme-wso2/issues"
  },
  "dependencies": {
    "theme-wso2": "file:module",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.2.2",
    "react-styleguidist": "6.0.33"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.2",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.2",
    "npm-watch": "^0.3.0",
    "react-scripts": "1.0.7",
    "react-styleguidist": "^6.0.33",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.3",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "stylelint-config-recommended-scss": "^2.0.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-order": "0.7.0",
    "webpack": "^3.6.0"
  },
  "eslintConfig": ".eslintrc",
  "sasslintConfig": ".sasslintrc",
  "watch": {
    "sasslint": {
      "patterns": [
        "src",
        "module"
      ],
      "extensions": "scss",
      "ignore": "node_modules/**/*.scss",
      "quiet": true,
      "legacyWatch": true,
      "delay": 500
    },
    "eslint": {
      "patterns": [
        "src",
        "module"
      ],
      "extensions": "js,jsx",
      "ignore": "node_modules/**/*.jsx,node_modules/**/*.js",
      "quiet": true,
      "legacyWatch": true,
      "delay": 500
    }
  },
  "scripts": {
    "dev": "cd module && npm link && cd .. && npm link theme-wso2 && webpack -d --watch",
    "buid": "webpack -p",
    "eslint": "eslint . --ext .jsx --ext .js",
    "sasslint": "sass-lint **/*.scss -v -q",
    "lint": "npm run eslint && npm run sasslint",
    "watch:eslint": "npm-watch eslint",
    "watch:sasslint": "npm-watch sasslint",
    "watch:lint": "npm-watch",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  }
}
